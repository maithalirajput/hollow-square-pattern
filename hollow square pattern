#include<stdio.h>

int main(){
    pattern(5);
}
void pattern(int n){
    for(int i=1; i<=n;i++){
        for(int j=1;j<=n;j++){
            if(i==1 || i==n || j==1 || j==5){
                printf(" %d ",j);
            }
            else{
                printf("   ");
            }
        }  
        printf("\n")     ;
    }
}
